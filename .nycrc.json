{
  "all": true,
  "check-coverage": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts"
  ],
  "reporter": [
    "text",
    "lcov"
  ],
  "skip-full": true,
  "skip-empty": true,
  "temp-dir": "coverage/tmp",
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "watermarks": {
    "lines": [
      99.9,
      99.0
    ],
    "functions": [
      99.9,
      99.0
    ],
    "branches": [
      99.9,
      99.0
    ],
    "statements": [
      99.9,
      99.0
    ]
  }
}